{
    "name": "ghaninia/captcha" ,
    "type": "package",
    "description": "A simple Laravel service provider for including the Captcha for Laravel.",
    "authors": [
        {
            "name": "Amin Ghaninia",
            "email": "info@ghaninia.ir"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0" ,
        "intervention/image": "^2.5"
    },
    "autoload": {
        "psr-4": {
            "GhaniniaIR\\Captcha\\" : "src/" 
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "GhaniniaIR\\Captcha\\CaptchaServiceProvider"
            ]
        }
    }
}
